{
  "name": "absolute-tms",
  "version": "1.0.0",
  "description": "Absolute TMS - Advanced Transportation Management System",
  "main": "server.js",
  "scripts": {
    "start": "node start.js",
    "server": "node server.js",
    "server-dev": "nodemon server.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server-dev\" \"npm run client\"",
    "build": "cd client && npm run build && cp -r build/* ../public/",
    "setup": "npm install && cd client && npm install",
    "demo": "node start.js",
    "deploy:aws": "serverless deploy",
    "deploy:aws-local": "serverless offline",
    "deploy:eb": "eb deploy"
  },
  "keywords": [
    "TMS",
    "Transportation",
    "Logistics",
    "MERN",
    "WebSocket"
  ],
  "author": "Absolute TMS Development Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.12.1",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "express-fileupload": "^1.5.2",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "google-auth-library": "^10.3.0",
    "googleapis": "^160.0.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.8.7",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.5",
    "pdfkit": "^0.13.0",
    "socket.io": "^4.7.2",
    "ssh2-sftp-client": "^12.0.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1",
    "serverless": "^3.38.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-http": "^3.2.0",
    "serverless-offline": "^13.7.0",
    "serverless-webpack": "^5.15.3",
    "webpack": "^5.101.3",
    "webpack-node-externals": "^3.0.0"
  }
}
